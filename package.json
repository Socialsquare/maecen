{
  "name": "maecen",
  "version": "0.0.0",
  "scripts": {
    "migrate:latest": "./node_modules/.bin/knex migrate:latest",
    "migrate:rollback": "./node_modules/.bin/knex migrate:rollback",
    "test": "",
    "test:server": "node_modules/.bin/cross-env NODE_ENV=testing PORT=3040 node_modules/.bin/ava ./server/tests/*.spec.js --serial",
    "test:server:watch": "node_modules/.bin/cross-env NODE_ENV=testing PORT=3040 node_modules/.bin/ava ./server/tests/*.spec.js --watch --serial",
    "start": "node_modules/.bin/cross-env NODE_ENV=development npm run migrate:latest && nodemon index.js",
    "start:prod": "node_modules/.bin/cross-env NODE_ENV=production node index.js",
    "bs": "npm run migrate:latest && npm run start:prod",
    "build": "node_modules/.bin/cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "node_modules/.bin/eslint client server shared migrations tests",
    "postinstall": "npm run build",
    "cron-job": "node ./tasks/cronJob.index.js"
  },
  "pre-commit": [
    "lint",
    "test:server"
  ],
